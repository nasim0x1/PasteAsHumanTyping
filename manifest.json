{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/icon_128_mc_4.png",
      "default_popup": "index.html"
   },
   "description": "Paste As Human Typing",
   "icons": {
      "128": "assets/icon_128_mc.png",
      "16": "assets/icon_128_mc_4.png"
   },
   
   "commands": {
      "paste_last": {
         "description": "Copy all saved copies at once",
         "suggested_key": {
            "default": "Ctrl+Q"
         }
      }
   },
   "manifest_version": 2,
   "name": "Human Typing By Nasim",
   "permissions": 
   [ 
      "activeTab",
       "storage", 
       "contextMenus"
       ],
   "version": "1.0.1"
}
